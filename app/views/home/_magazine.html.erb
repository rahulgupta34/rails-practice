<p>Display All The Magazines Here</p>

<table>
    <tr>
        <th>
            ID
        </th>
        <th>
            Name
        </th>
        <th>
            Created At
        </th>
        <th>
            Updated At
        </th>
    </tr>
    <tbody>
        <% @magazines.each do |magazine| %>
            <tr>
                <td><%= magazine.id %></td>
                <td><%= magazine.name %></td>
                <td><%= magazine.created_at.strftime("%Y:%m:%d") %></td>
                <td><%= magazine.updated_at.strftime("%Y:%m:%d") %></td>
               
            </tr>
            
        <% end %>
    </tbody>
</table>


